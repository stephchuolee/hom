<!DOCTYPE html>
<html>
<head>
  <title>Hom</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<nav>
  <ul>
    <li>
      <span>
        <%= form_tag "/listings/search", method: "get" do %>
        <%= label_tag(:city, "Search for a city:") %>
        <%= text_field_tag(:city) %>
        <%= submit_tag("Search") %>
        <% end %>
      </span>
    </li>
  </ul>
</nav>


<%= yield %>

</body>
</html>
